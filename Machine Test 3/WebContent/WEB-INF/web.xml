<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Machine Test 3</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>AddTraineeController</servlet-name>
    <servlet-class>com.TraineeMgt.web.controller.AddTraineeController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddTraineeController</servlet-name>
    <url-pattern>/addTrainee</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ShowTraineeController</servlet-name>
    <servlet-class>com.TraineeMgt.web.controller.ShowTraineeController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ShowTraineeController</servlet-name>
    <url-pattern>/showTrainee</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>LoggedInFilter</display-name>
    <filter-name>LoggedInFilter</filter-name>
    <filter-class>com.TraineeMgt.web.filter.LoggedInFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>LoggedInFilter</filter-name>
    <url-pattern>/LoggedInFilter</url-pattern>
  </filter-mapping>
</web-app>